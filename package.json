{
  "name": "speednote",
  "version": "2.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 3000",
    "lint": "eslint .",
    "clean": "rm -rf coverage dev-dist dist node_modules playwright-report test-results",
    "test": "vitest",
    "format-check": "prettier --check ./**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "test-ci": "vitest run --coverage",
    "quality-check": "pnpm format-check && pnpm lint && pnpm type-check",
    "e2e": "playwright test",
    "e2e-prod": "pnpm build && PRODUCTION_READY=1 pnpm e2e --ui",
    "e2e-ci": "pnpm build && PRODUCTION_READY=1 pnpm e2e"
  },
  "dependencies": {
    "@eslint/js": "^9.6.0",
    "@fontsource/inter": "^5.0.18",
    "@playwright/test": "^1.43.1",
    "@solid-primitives/scheduled": "^1.4.3",
    "@solidjs/router": "^0.13.6",
    "@solidjs/testing-library": "^0.8.8",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "20.12.7",
    "@vitest/coverage-v8": "^1.6.0",
    "eslint": "^9.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "jsdom": "^24.1.0",
    "prettier": "^3.2.5",
    "sass": "^1.75.0",
    "solid-devtools": "^0.30.1",
    "solid-js": "^1.8.17",
    "typescript": "5.4.5",
    "typescript-eslint": "^7.14.1",
    "vite": "^5.3.2",
    "vite-plugin-pwa": "^0.20.0",
    "vite-plugin-solid": "^2.10.2",
    "vitest": "^1.6.0",
    "zod": "^3.22.5"
  }
}
